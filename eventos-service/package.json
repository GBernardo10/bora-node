  },
  "devDependencies": {
    "@prisma/cli": "^2.0.1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.6",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^8.0.29",
    "@types/uuid": "^8.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "3.3.0",
    "ts-node-dev": "^1.0.0-pre.49",
